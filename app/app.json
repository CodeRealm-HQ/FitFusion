{
  "expo": {
    "name": "FitFusion",
    "slug": "FitFusion",
    "privacy": "unlisted",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/fusion.jpg",
    "splash": {
      "image": "./assets/splash.jpg",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "buildNumber": "1",
      "bundleIdentifier": "com.coderealm.fitfusion",
      "supportsTablet": true,
      "requireFullScreen": true
    },
    "android": {
      "versionCode": "1",
      "adaptiveIcon": {
        "foregroundImage": "./assets/fusion.jpg",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.coderealm.fitfusion"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
    [
      "expo-build-properties",
      {
        "ios": {
          "useFrameworks": "static"
        }
      }
    ]
    ]
  }
}
